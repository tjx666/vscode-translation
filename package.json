{
    "name": "vscode-translation",
    "displayName": "Translation",
    "description": "A translation tool for Visual Studio Code.",
    "version": "1.1.7",
    "publisher": "caiqichang",
    "engines": {
        "vscode": "^1.61.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "translate",
        "翻译",
        "发音",
        "pronunciation",
        "pronounce",
        "google",
        "谷歌"
    ],
    "icon": "resources/vscode-translation-icon.png",
    "extensionKind": [
        "ui",
        "workspace"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/caiqichang/vscode-translation"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onCommand:simpleTranslate",
        "onCommand:completeTranslate",
        "onCommand:translation",
        "onCommand:regularExpressionTest",
        "onCommand:ss",
        "onCommand:fileTemplate"
    ],
    "contributes": {
        "configuration": {
            "title": "Translation",
            "properties": {
                "translation.source-language": {
                    "description": "Default source language for translation.",
                    "type": "string",
                    "default": "auto"
                },
                "translation.target-language": {
                    "description": "Default target language for translation.",
                    "type": "string",
                    "default": "en"
                },
                "translation.enable-proxy": {
                    "description": "Controls whether to use http proxy for translation.",
                    "type": "boolean",
                    "default": false
                },
                "translation.proxy-url": {
                    "description": "URL for http proxy.",
                    "type": "string",
                    "default": "http://127.0.0.1:1080"
                },
                "translation.ss-enable-proxy": {
                    "description": "Controls whether to use http proxy for getting ss list.",
                    "type": "boolean",
                    "default": false
                },
                "translation.ss-enable-base64decode": {
                    "description": "Controls whether to decode ss result (base64).",
                    "type": "boolean",
                    "default": false
                },
                "translation.ss-urls": {
                    "description": "URLs array for getting ss list.",
                    "type": "array",
                    "default": []
                }
            }
        },
        "commands": [
            {
                "command": "simpleTranslate",
                "title": "Translate (Simple)"
            },
            {
                "command": "completeTranslate",
                "title": "Translate (Complete)"
            },
            {
                "command": "translation",
                "title": "Translation"
            },
            {
                "command": "regularExpressionTest",
                "title": "Regular Expression Test"
            },
            {
                "command": "ss",
                "title": "ss"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "simpleTranslate",
                    "when": "false"
                },
                {
                    "command": "completeTranslate",
                    "when": "false"
                }
            ],
            "editor/context": [
                {
                    "command": "simpleTranslate",
                    "when": "editorHasSelection",
                    "group": "0_translation@0"
                },
                {
                    "command": "completeTranslate",
                    "when": "editorHasSelection",
                    "group": "0_translation@1"
                }
            ]
        }
    },
    "scripts": {
        "prepackage": "node script/pre-package.js",
        "package": "npx --package vsce vsce package -o build/"
    },
    "devDependencies": {
        "@types/node": "14.x",
        "@types/vscode": "^1.61.0"
    },
    "dependencies": {
        "@vscode/codicons": "0.0.25",
        "@vscode/webview-ui-toolkit": "^0.8.2"
    }
}
